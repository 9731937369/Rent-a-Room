      <% Room.all.each do |room|%>
      <div class="col-md-4">
        <div class="card-box-a card-shadow">
          <div class="img-box-a">
            <%#= image_tag room.images ,:size => '500x450', class: "img-thumbnail img-a img-fluid" %>
